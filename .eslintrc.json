{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-console": 0,
        "no-unused-vars": 1,
        "quotes": [ 1, "double" ],
        "semi": [ 1, "always" ],
        "no-control-regex":  0
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "globals": {
        "sb": "readonly"
    }
}
